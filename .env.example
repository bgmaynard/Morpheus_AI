# Schwab API Configuration
SCHWAB_CLIENT_ID=your_client_id_here
SCHWAB_CLIENT_SECRET=your_client_secret_here
SCHWAB_REDIRECT_URI=https://localhost:8080/callback
SCHWAB_TOKEN_PATH=./tokens/schwab_token.json

# Runtime Configuration
MORPHEUS_ENV=development
MORPHEUS_LOG_LEVEL=INFO
MORPHEUS_EVENT_LOG_DIR=./logs/events

# Risk Configuration
MORPHEUS_MAX_DAILY_LOSS=500.0
MORPHEUS_MAX_POSITION_SIZE=0.05
MORPHEUS_MAX_TOTAL_EXPOSURE=0.25

# Meta Gate Configuration
MORPHEUS_META_GATE_ENABLED=true
MORPHEUS_META_MIN_CONFIDENCE=0.6
